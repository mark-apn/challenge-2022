  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run app",
      "cwd": "app",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart"
    },
    {
      "name": "Start worker",
      "cwd": "server",
      "request": "launch",
      "program": "bin/worker.dart",
      "type": "dart"
    },
    {
      "name": "Start server",
      "type": "dart",
      "request": "launch",
      "cwd": "server",
      "program": "bin/server.dart",
      "args": ["--port", "50000"]
    }
  ]
}
